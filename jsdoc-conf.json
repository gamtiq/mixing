{
    "source": {
        "include": ["index.js"]
    },

    "opts": {
        "destination": "docs",
        "readme": "README.md",
        "template": "node_modules/ink-docstrap/template",
        "fileSet": ".nojekyll"
    },

    "plugins": [
        "plugins/markdown",
        "jsdoc-file"
    ],

    "markdown": {
        "parser": "marked"
    },
    
    "templates": {
        "outputSourceFiles": true,
        "systemName": "mixing",
        "copyright": "Copyright (c) 2013-2021 Denis Sikuler",
        "theme": "spacelab",
        "linenums": true
    }
}
